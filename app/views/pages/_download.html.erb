      <CharStrings>
        <CharString name=".notdef">
          endchar
        </CharString>
        <% characters.each do |char| %>
        <% pointX = 0 %>
        <% pointY = 0 %>
        <CharString name="<%= char[:character] %>">
          <% char[:paths].each do |path| %>
            <% path.each_with_index do |p, i| %>
              <% if i > 0 %>
                <% if path[i - 1][0] == "M" %>
                  <% pointX = path[i - 1][1] %>
                  <% pointY = path[i - 1][2] %>
                <% else %>
                  <% pointX = path[i - 1][5] %>
                  <% pointY = path[i - 1][6] %>
                <% end %>
              <% end %>
              <% if p[0] == 'M' %>
                <%= "#{p[1] - pointX} #{p[2] - pointY} rmoveto" %>
                <% pointX = p[1] - pointX %>
                <% pointY = p[2] - pointY %>
              <% else %>
                <!-- <%= "#{p[1]} #{p[2]} #{p[3]} #{p[4]} #{p[5]} #{p[6]} rrcurveto" %> -->
                <%= "#{p[5] - pointX} #{p[6] - pointY} rlineto" %>
                <% pointX = p[5] - pointX %>
                <% pointY = p[6] - pointY %>
              <% end %> 
            <% end %>
          <% end if char[:paths].present? %>
          endchar
        </CharString>
        <% end %>
      </CharStrings>
    </CFFFont>

